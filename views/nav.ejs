<nav>
    <% if(isLoggedIn == 0) { %>
      <a href="/api/v1/auth/login">
         <button>Login</button>
      </a>
      <a href="/api/v1/auth/register">
         <button>Register</button>
      </a>
    <% } else { %>
      <form action="/api/v1/auth/logout" method="post">
         <input type="submit" id="submit" value="Logout">
      </form>

      <a href="/api/v1/products">
         <button>Cart</button>
      </a>
    <% } %>
</nav>